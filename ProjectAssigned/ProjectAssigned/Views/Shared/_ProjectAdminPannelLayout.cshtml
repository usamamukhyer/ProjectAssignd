@using Microsoft.AspNet.Identity
<!doctype html>
<html lang="en" dir="ltr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <meta name="msapplication-TileColor" content="#0f75ff">
    <meta name="theme-color" content="#9d37f6">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <link rel="icon" href="favicon.ico" type="image/x-icon" />
    <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />

    <!-- Title -->
    <title>Claylist – Clean & Modern Admin Dashboard Bootstrap 4  HTML Template</title>
    <link rel="stylesheet" href="~/Content/assets/fonts/fonts/font-awesome.min.css">

    <!-- Sidemenu Css -->
    <link href="~/Content/assets/plugins/toggle-sidebar/sidemenu.css" rel="stylesheet" />


    <!-- Bootstrap Css -->
    <link href="~/Content/assets/plugins/bootstrap-4.3.1-dist/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Dashboard Css -->
    <link href="~/Content/assets/css/dashboard.css" rel="stylesheet" />
    <link href="~/Content/assets/css/admin-custom.css" rel="stylesheet" />

    <!-- c3.js Charts Plugin -->
    <link href="~/Content/assets/plugins/charts-c3/c3-chart.css" rel="stylesheet" />
    <!-- select2 Plugin -->
    <link href="~/Content/assets/plugins/select2/select2.min.css" rel="stylesheet" />


    @*<link href="~/Content/assets/plugins/time-picker/jquery.timepicker.css" rel="stylesheet" />*@
    <!-- Date Picker Plugin -->
    <link href="~/Content/assets/plugins/date-picker/spectrum.css" rel="stylesheet" />
    <link href="~/Content/assets/plugins/datatable/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/assets/plugins/datatable/jquery.dataTables.min.css" rel="stylesheet" />

    <!-- Custom scroll bar css-->
    <link href="~/Content/assets/plugins/scroll-bar/jquery.mCustomScrollbar.css" rel="stylesheet" />
    <!-- file Uploads -->
    <link href="~/Content/assets/plugins/fileuploads/css/dropify.css" rel="stylesheet" type="text/css" />
    <!---Font icons-->
    <link href="~/Content/assets/css/icons.css" rel="stylesheet" />
    <link href="~/Content/toastr.css" rel="stylesheet" />

</head>
<body class="app sidebar-mini">
    <!--Loader-->
    <div id="global-loader">
        <img src="~/Content/assets/images/products/products/loader.png" class="loader-img floating" alt="">
    </div>
    <div class="page">
        <div class="page-main">
            <div class="app-header1 header py-1 d-flex">
                <div class="container-fluid">
                    <div class="d-flex">
                        <a class="header-brand" href="index.html">
                            <img src="~/Content/assets/images/brand/logo.png" class="header-brand-img" alt="Claylist logo">
                        </a>
                        <a aria-label="Hide Sidebar" class="app-sidebar__toggle" data-toggle="sidebar" href="#"></a>






                        @{ ProjectAssigned.Models.ProjectAssignedEntities db = new ProjectAssigned.Models.ProjectAssignedEntities();
                            string Image = "";
                            string userId = (new ProjectAssigned.Models.GenericModel()).FetchUserProfile().Id;
                            var userpic = db.AspNetUsers.FirstOrDefault(x => x.Id == userId);
                            if (userpic != null)
                            {
                                Image = userpic.Photo;

                            }
                            else
                            {
                                Image = "dummy-avatar.png";
                            } }





                        <div class="d-flex order-lg-2 ml-auto">
                            <div class="dropdown">


                                <a href="#" class="nav-link pr-0 leading-none user-img" data-toggle="dropdown">
                                    <span class="username">@User.Identity.GetUserName()</span>
                                    <img src="~/Content/Filesdata/Images/@Image" alt="profile-img" class="avatar avatar-md brround">
                                </a>
                                <div class="dropdown-menu dropdown-menu-md-right dropdown-menu-arrow ">
                                    <a class="dropdown-item" href="@Url.Action("ResetPassword","Account")">
                                        <i class="dropdown-icon icon icon-user"></i> Change Password
                                    </a>

                                    <a class="dropdown-item" href="javascript:document.getElementById('logoutForm').submit()">
                                        <i class="dropdown-icon icon icon-power"></i> Log out
                                    </a>
                                    @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                                    {
                        @Html.AntiForgeryToken()}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Sidebar menu-->
            <div class="app-sidebar__overlay" data-toggle="sidebar"></div>
            <aside class="app-sidebar doc-sidebar">
                <div class="app-sidebar__user clearfix">
                    <div class="dropdown user-pro-body">
                        <div>
                            <img src="~/Content/Filesdata/Images/@Image" alt="user-img" class="avatar avatar-lg brround">
                            <a href="editprofile.html" class="profile-img">
                                <span class="fa fa-pencil" aria-hidden="true"></span>
                            </a>
                        </div>
                        <div class="user-info">
                            <h2>@userpic.FirstName  @userpic.LastName</h2>
                            <span>
                              @userpic.Designation
                            </span>
                        </div>
                    </div>
                </div>
              
                <ul class="side-menu">
                    <li class="slide">
                        <a class="side-menu__item" href="@Url.Action("Progress", "DashBoard")"><i class="side-menu__icon fa fa-tachometer"></i><span class="side-menu__label">Dashboard</span></a>
                    </li>
                    <li class="slide">
                        <a class="side-menu__item" data-toggle="slide" href="#"><i class="side-menu__icon fa fa-user"></i><span class="side-menu__label">Developers</span><i class="angle fa fa-angle-right"></i></a>
                        <ul class="slide-menu">
                            <li>
                                <a href="@Url.Action("AddDeveloper", "CreateDeveloper")" class="slide-item">Create Developer</a>

                            </li>

                            <li class="slide">
                                <a href="@Url.Action("DeveloperList", "CreateDeveloper")" class="slide-item">Developers List</a>
                            </li>
                        </ul>
                    </li>

                    <li class="slide">
                        <a class="side-menu__item" data-toggle="slide" href="#"><i class="side-menu__icon fa fa-wpforms"></i><span class="side-menu__label">Projects</span><i class="angle fa fa-angle-right"></i></a>
                        <ul class="slide-menu">
                            <li>
                                <a href="@Url.Action("AddProject", "AssignProject")" class="slide-item">Assign Project</a>
                            </li>

                            <li class="slide">
                                <a href="@Url.Action("Index", "AssignProject")" class="slide-item">Project List</a>
                            </li>
                        </ul>
                    </li>

                    <li class="slide">
                        <a class="side-menu__item" href="@Url.Action("Index", "Admin")"><i class="side-menu__icon fa fa-user-circle"></i><span class="side-menu__label">Admin Manage Form</span></a>

                    </li>

                    <li class="slide">
                        <a class="side-menu__item" href="@Url.Action("Assigntable", "DeveloperAssignList")"><i class="side-menu__icon fa fa-user"></i><span class="side-menu__label">Developer Manage Form</span></a>

                    </li>
                    <li class="slide">
                        <a class="side-menu__item" data-toggle="slide" href="#"><i class="side-menu__icon fa fa-edit"></i><span class="side-menu__label">Modules</span><i class="angle fa fa-angle-right"></i></a>
                        <ul class="slide-menu">
                            <li>
                                <a href="@Url.Action("AddModule", "NewModules")" class="slide-item">Add Module</a>
                            </li>

                            <li class="slide">
                                <a href="@Url.Action("Index", "NewModules")" class="slide-item">Module List</a>
                            </li>
                        </ul>
                    </li>

                    <li class="slide">
                        <a class="side-menu__item" data-toggle="slide" href="#"><i class="side-menu__icon fa fa-money"></i><span class="side-menu__label">Company Income</span><i class="angle fa fa-angle-right"></i></a>
                        <ul class="slide-menu">
                            <li>
                                <a href="@Url.Action("AddTransection", "Transection")" class="slide-item">Add Cash</a>
                            </li>

                            <li class="slide">
                                <a href="@Url.Action("Index", "Transection")" class="slide-item">Cash List</a>
                            </li>
                        </ul>
                    </li>



                </ul>
            
            </aside>
        </div>
        <div>
            @RenderBody();
        </div>
        <!--footer-->
        <footer class="footer">
            <div class="container">
                <div class="row align-items-center flex-row-reverse">
                    <div class="col-lg-12 col-sm-12 mt-3 mt-lg-0 text-center">
                        Copyright © 2019 <a href="#">Claylist</a>. Designed by <a href="#">Spruko</a> All rights reserved.
                    </div>
                </div>
            </div>
        </footer>
        <!-- End Footer-->
    </div>

    <!-- Back to top -->
    <a href="#top" id="back-to-top"><i class="fa fa-rocket"></i></a>


    <!-- Dashboard js -->
    <script src="~/Content/assets/js/vendors/jquery-3.2.1.min.js"></script>
    <script src="~/Content/assets/plugins/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/Content/assets/plugins/jquery-validation/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/toastr.js"></script>
    <script src="~/Scripts/generic.js"></script>
    <script src="~/Content/assets/plugins/bootstrap-4.3.1-dist/js/popper.min.js"></script>
    <script src="~/Content/assets/plugins/bootstrap-4.3.1-dist/js/bootstrap.min.js"></script>
    @*<script src="~/Content/assets/js/vendors/jquery.sparkline.min.js"></script>
        <script src="~/Content/assets/js/vendors/selectize.min.js"></script>
        <script src="~/Content/assets/js/vendors/jquery.tablesorter.min.js"></script>
        <script src="~/Content/assets/js/vendors/circle-progress.min.js"></script>
        <script src="~/Content/assets/plugins/rating/jquery.rating-stars.js"></script>*@

    <!-- Fullside-menu Js-->
    <script src="~/Content/assets/plugins/toggle-sidebar/sidemenu.js"></script>
    <!--Select2 js -->
    <script src="~/Content/assets/plugins/select2/select2.full.min.js"></script>
    <!-- Timepicker js -->
    <!--<script src="~/Content/assets/plugins/time-picker/jquery.timepicker.js"></script>
    <script src="~/Content/assets/plugins/time-picker/toggles.min.js"></script>-->
    <!-- Data tables -->
    <script src="~/Content/assets/plugins/datatable/jquery.dataTables.min.js"></script>
    <script src="~/Content/assets/plugins/datatable/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/assets/js/datatable.js"></script>

    <!-- Datepicker js -->
    <script src="~/Content/assets/plugins/date-picker/spectrum.js"></script>
    <script src="~/Content/assets/plugins/date-picker/jquery-ui.js"></script>
    @*<script src="~/Content/assets/plugins/input-mask/jquery.maskedinput.js"></script>*@
    <!-- Inline js -->
    @*<script src="~/Content/assets/js/select2.js"></script>*@
    <script src="~/Content/assets/js/formelements.js"></script>

    <!-- file uploads js -->
    <script src="~/Content/assets/plugins/fileuploads/js/dropify.js"></script>

    <!-- Index Scripts-->
    <script src="~/Content/assets/js/index2.js"></script>


    <!--Morris.js Charts Plugin -->
    <script src="~/Content/assets/plugins/morris/raphael-min.js"></script>
    <script src="~/Content/assets/plugins/morris/morris.js"></script>




    <!-- Custom scroll bar Js-->
    <!--<script src="~/Content/assets/plugins/scroll-bar/jquery.mCustomScrollbar.concat.min.js"></script>-->
    @*<script src="~/Content/assets/plugins/counters/counterup.min.js"></script>
        <script src="~/Content/assets/plugins/counters/waypoints.min.js"></script>*@

    <!-- Custom Js-->
    <script src="~/Content/assets/js/admin-custom.js"></script>



    @RenderSection("Mysection", required: false)

</body>
</html>
